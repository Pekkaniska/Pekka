    <use>
      <common>true</common>
      <for>
        <role>c4d67026-2c97-4126-b8be-49aa6f567e2d</role>
      </for>
      <for>
        <role>Role.БазовыеПраваБСП</role>
      </for>
      <for>
        <role>Role.БазовыеПраваУТ</role>
      </for>
      <for>
