					ЗначениеАтрибута = Неопределено;
					Если ЗначениеЗаполнено(СтрокаШаблона.Ключ) Тогда
						ИмяПоказателя = СтрокаШаблона.Ключ + "_" + СтрокаЧГ0(НомерСтроки);
						ЗначениеАтрибута = Сведения[ИмяПоказателя];
