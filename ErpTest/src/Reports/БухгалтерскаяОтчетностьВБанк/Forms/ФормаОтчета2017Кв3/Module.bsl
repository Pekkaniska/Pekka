		СтрокаРаздела = ДеревоСтраниц.Строки.Найти(ЗаполняемыйРаздел, "ИмяСтраницы", Истина);
		СтрокаРаздела.ЗаполняетсяПоИБ = Истина;
	КонецЦикла;
	
	ЗначениеВРеквизитФормы(ДеревоСтраниц, "мДеревоСтраницОтчета");
	
