	КОНЕЦ КАК КПП,
	ВложенныйЗапрос.ПробегГруз,
	ВложенныйЗапрос.ВесГруза,
	ВложенныйЗапрос.ТнКм,
	ВложенныйЗапрос.КоличествоЕздок,
	ВложенныйЗапрос.РасходПоНорме,
	ВложенныйЗапрос.РасходПоФакту,
	ВЫБОР
		КОГДА ВложенныйЗапрос.РасходПоНорме - ВложенныйЗапрос.РасходПоФакту &gt; 0
			ТОГДА ВложенныйЗапрос.РасходПоНорме - ВложенныйЗапрос.РасходПоФакту
		ИНАЧЕ 0
	КОНЕЦ КАК Экономия,
	ВЫБОР
		КОГДА ВложенныйЗапрос.РасходПоНорме - ВложенныйЗапрос.РасходПоФакту &gt; 0
			ТОГДА 0
		ИНАЧЕ ВложенныйЗапрос.РасходПоФакту - ВложенныйЗапрос.РасходПоНорме
	КОНЕЦ КАК Пережог,
	ВложенныйЗапрос.ВремяВНаряде,
	ВложенныйЗапрос.ВремяВРаботе,
	уатМестонахождениеТССрезПоследних.Организация,
	уатМестонахождениеТССрезПоследних.Колонна,
	ЕСТЬNULL(уатПраваИНастройки.Значение, 0) КАК Точность
ИЗ
	Справочник.ТранспортныеСредства КАК СправочникТранспортныеСредства
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВложенныйЗапросВыработка.ТС КАК ТС,
			СУММА(ВложенныйЗапросВыработка.ПробегОбщий) КАК ПробегОбщий,
			СУММА(ВложенныйЗапросВыработка.РасходПоФакту) КАК РасходПоФакту,
			СУММА(ВложенныйЗапросВыработка.ВремяВРемонте) КАК ВремяВРемонте,
			СУММА(ВложенныйЗапросВыработка.ПробегГруз) КАК ПробегГруз,
			СУММА(ВложенныйЗапросВыработка.ВесГруза) КАК ВесГруза,
			СУММА(ВложенныйЗапросВыработка.ТнКм) КАК ТнКм,
			СУММА(ВложенныйЗапросВыработка.КоличествоЕздок) КАК КоличествоЕздок,
			СУММА(ВложенныйЗапросВыработка.РасходПоНорме) КАК РасходПоНорме,
			СУММА(ВложенныйЗапросВыработка.ВремяВНаряде) КАК ВремяВНаряде,
			СУММА(ВложенныйЗапросВыработка.ВремяВРаботе) КАК ВремяВРаботе
		ИЗ
			(ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС КАК ТС,
				уатВыработкаТСОбороты.КоличествоОборот КАК ПробегОбщий,
				NULL КАК РасходПоФакту,
				NULL КАК ВремяВРемонте,
				NULL КАК ПробегГруз,
				NULL КАК ВесГруза,
				NULL КАК ТнКм,
				NULL КАК КоличествоЕздок,
				NULL КАК РасходПоНорме,
				NULL КАК ВремяВНаряде,
				NULL КАК ВремяВРаботе
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ПробегОбщий)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ПробегСГрузом)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ВесГруза)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL,
				NULL,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ТнКм)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.КоличествоЕздок)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ВремяВРемонте)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатРасходГСМнаТСОбороты.ТС,
				NULL,
				уатРасходГСМнаТСОбороты.РасходПоФактуОборот,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				уатРасходГСМнаТСОбороты.РасходПоНормеОборот,
				NULL,
				NULL
			ИЗ
				РегистрНакопления.уатРасходГСМнаТС.Обороты(, , , ) КАК уатРасходГСМнаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот,
				NULL
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ВремяВНаряде)) КАК уатВыработкаТСОбороты
			
			ОБЪЕДИНИТЬ ВСЕ
			
			ВЫБРАТЬ
				уатВыработкаТСОбороты.ТС,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				NULL,
				уатВыработкаТСОбороты.КоличествоОборот
			ИЗ
				РегистрНакопления.уатВыработкаТС.Обороты(, , , ПараметрВыработки = ЗНАЧЕНИЕ(Справочник.уатПараметрыВыработки.ВремяВРаботе)) КАК уатВыработкаТСОбороты) КАК ВложенныйЗапросВыработка
		
		СГРУППИРОВАТЬ ПО
			ВложенныйЗапросВыработка.ТС) КАК ВложенныйЗапрос
		ПО (ВложенныйЗапрос.ТС = СправочникТранспортныеСредства.Ссылка)
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних КАК уатМестонахождениеТССрезПоследних
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатПраваИНастройки КАК уатПраваИНастройки
			ПО (уатПраваИНастройки.ПравоНастройка = ЗНАЧЕНИЕ(ПланВидовХарактеристик.уатПраваИНастройки.ТочностьОстатковТоплива))
				И уатМестонахождениеТССрезПоследних.Организация = уатПраваИНастройки.Объект
		ПО СправочникТранспортныеСредства.Ссылка = уатМестонахождениеТССрезПоследних.ТС
ГДЕ
	(&amp;ОтображатьВыбывшиеТС
			ИЛИ СправочникТранспортныеСредства.уатДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1))
	И (&amp;Организация = ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
			ИЛИ &amp;Организация = НЕОПРЕДЕЛЕНО
			ИЛИ уатМестонахождениеТССрезПоследних.Организация = &amp;Организация)</query>
	</dataSet>
