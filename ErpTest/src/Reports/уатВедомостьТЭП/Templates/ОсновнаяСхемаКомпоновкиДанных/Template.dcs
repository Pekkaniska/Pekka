	<totalField>
		<dataPath>ВремяВНаряде</dataPath>
		<expression>ВЫБОР
        КОГДА &amp;ВыводитьВремяВФорматеЧЧММ
            ТОГДА ВЫРАЗИТЬ((СУММА(ВремяВНаряде) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1
            + (СУММА(ВремяВНаряде) / 60
            - (ВЫРАЗИТЬ((СУММА(ВремяВНаряде) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1) * 60)/100
        ИНАЧЕ СУММА(ВремяВНаряде) / 3600
    КОНЕЦ</expression>
	</totalField>
	<totalField>
		<dataPath>ВремяВРаботе</dataPath>
		<expression>ВЫБОР
        КОГДА &amp;ВыводитьВремяВФорматеЧЧММ
            ТОГДА ВЫРАЗИТЬ((СУММА(ВремяВРаботе) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1
            + (СУММА(ВремяВРаботе) / 60
            - (ВЫРАЗИТЬ((СУММА(ВремяВРаботе) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1) * 60)/100
        ИНАЧЕ СУММА(ВремяВРаботе) / 3600
    КОНЕЦ</expression>
	</totalField>
	<totalField>
		<dataPath>ВремяВРемонте</dataPath>
		<expression>ВЫБОР
        КОГДА &amp;ВыводитьВремяВФорматеЧЧММ
            ТОГДА ВЫРАЗИТЬ((СУММА(ВремяВРемонте) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1
            + (СУММА(ВремяВРемонте) / 60
            - (ВЫРАЗИТЬ((СУММА(ВремяВРемонте) / 3600 + 0.5), "ЧИСЛО(15, 0)") - 1) * 60)/100
        ИНАЧЕ СУММА(ВремяВРемонте) / 3600
    КОНЕЦ</expression>
	</totalField>
