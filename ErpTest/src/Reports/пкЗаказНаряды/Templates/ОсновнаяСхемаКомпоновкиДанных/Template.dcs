		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	пкЗаказНаряд.Ссылка,
	пкЗаказНаряд.Номер,
	пкЗаказНаряд.Дата,
	пкЗаказНаряд.Техника,
	пкЗаказНаряд.ВидРемонта,
	пкЗаказНаряд.Статус,
	ВЫБОР
		КОГДА пкЗаказНаряд.МестоРемонта = ЗНАЧЕНИЕ(Перечисление.пкМестоРемонта.УКлиента)
			ТОГДА пкЗаказНаряд.Клиент
	КОНЕЦ КАК Клиент,
	ВЫБОР
		КОГДА пкЗаказНаряд.МестоРемонта = ЗНАЧЕНИЕ(Перечисление.пкМестоРемонта.УКлиента)
			ТОГДА пкЗаказНаряд.ОбъектСтроительства
	КОНЕЦ КАК ОбъектСтроительства,
	пкЗаказНаряд.Моточасы,
	пкЗаказНаряд.ТемператураЗамерзанияОЖ,
	пкЗаказНаряд.ТехникаКлиента,
	пкЗаказНаряд.ГарантийныйРемонт,
	пкЗаказНаряд.ПлатныйРемонт,
	пкЗаказНаряд.ПлановаяКалькуляция,
	пкЗаказНаряд.Подразделение
ПОМЕСТИТЬ ВТ_ЗаказНаряды
ИЗ
	Документ.пкЗаказНаряд КАК пкЗаказНаряд
ГДЕ
	пкЗаказНаряд.Дата &gt;= &amp;НачалоПериода
	И (пкЗаказНаряд.Дата &lt;= &amp;КонецПериода
			ИЛИ &amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1))
	И пкЗаказНаряд.Проведен
	И НЕ пкЗаказНаряд.ПометкаУдаления
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(пкЗаказНарядМатериалы.Количество) КАК Количество,
	пкЗаказНарядМатериалы.Ссылка,
	пкЗаказНарядМатериалы.Номенклатура
ПОМЕСТИТЬ ВТ_Материалы
ИЗ
	Документ.пкЗаказНаряд.Материалы КАК пкЗаказНарядМатериалы
ГДЕ
	пкЗаказНарядМатериалы.Ссылка В
			(ВЫБРАТЬ
				ВТ_ЗаказНаряды.Ссылка
			ИЗ
				ВТ_ЗаказНаряды)

СГРУППИРОВАТЬ ПО
	пкЗаказНарядМатериалы.Ссылка,
	пкЗаказНарядМатериалы.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	пкЗаказНарядРаботы.Ссылка,
	СУММА(пкЗаказНарядРаботы.Количество) КАК Количество,
	пкЗаказНарядРаботы.Номенклатура,
	пкЗаказНарядРаботы.Сотрудник
ПОМЕСТИТЬ ВТ_Работы
ИЗ
	Документ.пкЗаказНаряд.Работы КАК пкЗаказНарядРаботы
ГДЕ
	пкЗаказНарядРаботы.Ссылка В
			(ВЫБРАТЬ
				ВТ_ЗаказНаряды.Ссылка
			ИЗ
				ВТ_ЗаказНаряды)

СГРУППИРОВАТЬ ПО
	пкЗаказНарядРаботы.Ссылка,
	пкЗаказНарядРаботы.Номенклатура,
	пкЗаказНарядРаботы.Сотрудник
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ЗаказНаряды.Ссылка КАК ЗаказНаряд,
	ВТ_ЗаказНаряды.Ссылка КАК НомерЗН,
	ВТ_ЗаказНаряды.Ссылка КАК ДатаЗН,
	ВТ_ЗаказНаряды.Техника,
	ВТ_ЗаказНаряды.ВидРемонта,
	ВТ_ЗаказНаряды.Статус,
	ВТ_ЗаказНаряды.Клиент,
	ВТ_ЗаказНаряды.ОбъектСтроительства,
	ВЫБОР
		КОГДА ВложенныйЗапрос.ЭтоМатериал
			ТОГДА ВложенныйЗапрос.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК ЗапчастиКоличество,
	ВЫБОР
		КОГДА ВложенныйЗапрос.ЭтоМатериал
			ТОГДА 0
		ИНАЧЕ ВложенныйЗапрос.Количество
	КОНЕЦ КАК КоличествоЧасов,
	ВТ_ЗаказНаряды.Моточасы,
	ВТ_ЗаказНаряды.ТемператураЗамерзанияОЖ,
	ВТ_ЗаказНаряды.ТехникаКлиента,
	ВТ_ЗаказНаряды.ГарантийныйРемонт,
	ВТ_ЗаказНаряды.ПлатныйРемонт,
	ВТ_ЗаказНаряды.ПлановаяКалькуляция,
	ВложенныйЗапрос.Номенклатура КАК Номенклатура,
	ВложенныйЗапрос.ЭтоМатериал,
	ВложенныйЗапрос.Механник КАК Механник,
	ВТ_ЗаказНаряды.Подразделение
ИЗ
	ВТ_ЗаказНаряды КАК ВТ_ЗаказНаряды
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВТ_Материалы.Количество КАК Количество,
			ВТ_Материалы.Ссылка КАК Ссылка,
			ВТ_Материалы.Номенклатура КАК Номенклатура,
			ИСТИНА КАК ЭтоМатериал,
			Null КАК Механник
		ИЗ
			ВТ_Материалы КАК ВТ_Материалы
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ВТ_Работы.Количество,
			ВТ_Работы.Ссылка,
			ВТ_Работы.Номенклатура,
			ЛОЖЬ,
			ВТ_Работы.Сотрудник
		ИЗ
			ВТ_Работы КАК ВТ_Работы) КАК ВложенныйЗапрос
		ПО ВТ_ЗаказНаряды.Ссылка = ВложенныйЗапрос.Ссылка</query>
