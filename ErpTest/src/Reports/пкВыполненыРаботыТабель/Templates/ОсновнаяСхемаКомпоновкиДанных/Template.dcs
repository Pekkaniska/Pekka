	РегистрНакопления.пкВыполненныеРаботы.Обороты({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, , ) КАК пкВыполненныеРаботыОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.пкЗаказНаряд КАК пкЗаказНаряд
		ПО пкВыполненныеРаботыОбороты.ЗаказНаряд = пкЗаказНаряд.Ссылка
{ГДЕ
	пкВыполненныеРаботыОбороты.ЗаказНаряд.*,
	пкВыполненныеРаботыОбороты.Работа.*,
	пкВыполненныеРаботыОбороты.Сотрудник.*,
	пкЗаказНаряд.Техника.*,
	пкЗаказНаряд.Подразделение.*}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВТ_Обороты.Сотрудник КАК Сотрудник,
	ВТ_Обороты.Подразделение КАК Подразделение
ПОМЕСТИТЬ ВТ_Подразделения
ИЗ
	ВТ_Обороты КАК ВТ_Обороты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Обороты.Подразделение КАК РаботыПодразделение,
	ВТ_Обороты.Техника КАК РаботыТехника,
	ВТ_Обороты.Сотрудник КАК РаботыСотрудник,
	ВТ_Обороты.ЗаказНаряд КАК РаботыЗаказНаряд,
	ВТ_Обороты.Работа КАК РаботыРабота,
	пкВыполненныеРаботы.Количество КАК РаботыКоличество,
	пкВыполненныеРаботы.Сумма КАК РаботыСумма,
	пкВыполненныеРаботы.ДатаНачала КАК РаботыДатаНачала,
	пкВыполненныеРаботы.ДатаОкончания КАК РаботыДатаОкончания,
	пкВыполненныеРаботы.Командировка КАК РаботыКомандировка,
	НАЧАЛОПЕРИОДА(пкВыполненныеРаботы.ДатаОкончания, ДЕНЬ) КАК РаботыДатаРаботы,
	ВЫБОР
		КОГДА пкВыполненныеРаботы.Командировка
			ТОГДА пкВыполненныеРаботы.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК РаботыЧасыКомандировка,
	ВЫБОР
		КОГДА пкВыполненныеРаботы.НочныеРаботы
			ТОГДА пкВыполненныеРаботы.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК РаботыЧасыНочные,
	ВЫБОР
		КОГДА пкВыполненныеРаботы.ВыходныеДни
			ТОГДА пкВыполненныеРаботы.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК РаботыЧасыВыходные,
	пкВыполненныеРаботы.Количество - ВЫБОР
		КОГДА пкВыполненныеРаботы.ВыходныеДни
			ТОГДА пкВыполненныеРаботы.Количество
		ИНАЧЕ 0
	КОНЕЦ КАК РаботыЧасыБудни,
	пкВыполненныеРаботы.Количество КАК РаботыЧасыПраздничные,
	0 КАК РаботыДниКомандировки,
	NULL КАК РаботыКоличествоПлан,
	NULL КАК РаботыКоличествоФакт
ПОМЕСТИТЬ ВТ_Данные
ИЗ
	ВТ_Обороты КАК ВТ_Обороты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.пкВыполненныеРаботы КАК пкВыполненныеРаботы
		ПО ВТ_Обороты.ЗаказНаряд = пкВыполненныеРаботы.ЗаказНаряд
			И ВТ_Обороты.Работа = пкВыполненныеРаботы.Работа
			И ВТ_Обороты.Сотрудник = пкВыполненныеРаботы.Сотрудник
			И (НАЧАЛОПЕРИОДА(пкВыполненныеРаботы.ДатаОкончания, ДЕНЬ) &gt;= &amp;НачалоПериода)
			И (НАЧАЛОПЕРИОДА(пкВыполненныеРаботы.ДатаОкончания, ДЕНЬ) &lt;= &amp;КонецПериода
				ИЛИ &amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1))

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	пкВыполненныеРаботыК.Подразделение,
	NULL,
	пкВыполненныеРаботыК.Сотрудник,
	NULL,
	NULL,
	0,
	0,
	NULL,
	NULL,
	NULL,
	НАЧАЛОПЕРИОДА(пкВыполненныеРаботыК.ДатаОкончания, ДЕНЬ),
	0,
	0,
	0,
	0,
	0,
	ВЫБОР
		КОГДА пкВыполненныеРаботыК.Командировка
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ,
	NULL,
	NULL
ИЗ
	(ВЫБРАТЬ
		пкЗаказНаряд.Подразделение КАК Подразделение,
		пкВыполненныеРаботыЗаписи.Сотрудник КАК Сотрудник,
		НАЧАЛОПЕРИОДА(пкВыполненныеРаботыЗаписи.ДатаОкончания, ДЕНЬ) КАК ДатаОкончания,
		пкВыполненныеРаботыЗаписи.Командировка КАК Командировка
	ИЗ
		РегистрНакопления.пкВыполненныеРаботы КАК пкВыполненныеРаботыЗаписи
			ЛЕВОЕ СОЕДИНЕНИЕ Документ.пкЗаказНаряд КАК пкЗаказНаряд
			ПО пкВыполненныеРаботыЗаписи.ЗаказНаряд = пкЗаказНаряд.Ссылка
	ГДЕ
		пкВыполненныеРаботыЗаписи.Командировка
		И НАЧАЛОПЕРИОДА(пкВыполненныеРаботыЗаписи.ДатаОкончания, ДЕНЬ) &gt;= &amp;НачалоПериода
		И (НАЧАЛОПЕРИОДА(пкВыполненныеРаботыЗаписи.ДатаОкончания, ДЕНЬ) &lt;= &amp;КонецПериода
				ИЛИ &amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1))
	
	СГРУППИРОВАТЬ ПО
		пкЗаказНаряд.Подразделение,
		пкВыполненныеРаботыЗаписи.Сотрудник,
		НАЧАЛОПЕРИОДА(пкВыполненныеРаботыЗаписи.ДатаОкончания, ДЕНЬ),
		пкВыполненныеРаботыЗаписи.Командировка) КАК пкВыполненныеРаботыК

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_Подразделения.Подразделение,
	NULL,
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.Сотрудник,
	"Табель",
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ВидУчетаВремени,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.Период,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	СУММА(ВЫБОР
			КОГДА пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ВидВремени = ЗНАЧЕНИЕ(Перечисление.пкВидВремени.План)
				ТОГДА пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ЧасыОборот
		КОНЕЦ),
	СУММА(ВЫБОР
			КОГДА пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ВидВремени = ЗНАЧЕНИЕ(Перечисление.пкВидВремени.Факт)
				ТОГДА пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ЧасыОборот
		КОНЕЦ)
ИЗ
	РегистрНакопления.пкДанныеУчетаРабочегоВремениСотрудников.Обороты({(&amp;НачалоПериода)}, {(&amp;КонецПериода)}, Регистратор, ) КАК пкДанныеУчетаРабочегоВремениСотрудниковОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Подразделения КАК ВТ_Подразделения
		ПО (ВТ_Подразделения.Сотрудник = пкДанныеУчетаРабочегоВремениСотрудниковОбороты.Сотрудник)

СГРУППИРОВАТЬ ПО
	ВТ_Подразделения.Подразделение,
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.Сотрудник,
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.ВидУчетаВремени,
	пкДанныеУчетаРабочегоВремениСотрудниковОбороты.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Данные.РаботыПодразделение КАК РаботыПодразделение,
	ВТ_Данные.РаботыТехника КАК РаботыТехника,
	ВТ_Данные.РаботыСотрудник КАК РаботыСотрудник,
