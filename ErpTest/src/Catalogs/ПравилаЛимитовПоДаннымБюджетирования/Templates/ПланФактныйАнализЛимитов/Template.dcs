		<expression>ВЫБОР
	КОГДА 
		ВЫБОР 
			КОГДА ЕстьNull(Среднее(СуммаПлан), 0) = 0
				ТОГДА 0
			КОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0) &lt;&gt; 0
				ТОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0)
			КОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0) &lt;&gt; 0
				ТОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0)
		КОНЕЦ = 0
		ТОГДА ЕстьNull(Сумма(СуммаФакт), 0)
	КОГДА 
		ВЫБОР 
			КОГДА ЕстьNull(Среднее(СуммаПлан), 0) = 0
				ТОГДА 0
			КОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0) &lt;&gt; 0
				ТОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0)
			КОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0) &lt;&gt; 0
				ТОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0)
		КОНЕЦ &lt; ЕстьNull(Сумма(СуммаФакт), 0)
		ТОГДА ЕстьNull(Сумма(СуммаФакт), 0) - 
			ВЫБОР 
				КОГДА ЕстьNull(Среднее(СуммаПлан), 0) = 0
					ТОГДА 0
				КОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0) &lt;&gt; 0
					ТОГДА ЕстьNull(Среднее(СуммаПланПоКонтрольнойАналитике), 0)
				КОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0) &lt;&gt; 0
					ТОГДА ЕстьNull(Среднее(СуммаПланПоПрочейАналитике), 0)
